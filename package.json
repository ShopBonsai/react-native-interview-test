{
  "name": "interview",
  "version": "1.0.0",
  "repository": "git@github.com:shopbonsai/interview-test-native.git",
  "author": "Mackie Drew <mackie@shopbonsai.ca>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "lint": "tsc --noEmit --skipLibCheck && eslint --ext .js,.jsx,.ts,.tsx ./",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "e2e": "yarn run e2e-ios",
    "e2e-ios": "detox build -c ios.sim.debug && detox test -c ios.sim.debug",
    "e2e-android": "npx jetify && detox build -c android.emu.debug && detox test -c android.emu.debug",
    "precommit": "lint-staged && yarn test --lastCommit",
    "devtools": "react-devtools",
    "clear-cache": "watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-* && rm -rf node_modules && yarn && rm -rf ios/build && rm -rf android/build",
    "prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "axios": "^0.19.0",
    "get-value": "3.0.1",
    "jetifier": "^1.6.4",
    "react": "^16.9.0",
    "react-native": "^0.60.5",
    "react-native-flash-message": "^0.1.13",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-modal": "^11.3.1",
    "react-native-navigation": "^3.2.0",
    "react-native-raw-bottom-sheet": "^2.0.2",
    "react-native-reanimated": "^1.2.0",
    "react-native-screens": "^2.0.0-alpha.3",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "4.0.0",
    "react-navigation-stack": "^1.7.3",
    "react-navigation-tabs": "^2.5.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.0.5",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "@storybook/addon-actions": "5.1.11",
    "@storybook/addon-links": "5.1.11",
    "@storybook/addon-storyshots": "5.1.11",
    "@storybook/addons": "5.1.11",
    "@storybook/react-native": "5.1.11",
    "@types/axios": "^0.14.0",
    "@types/detox": "^12.8.4",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/react-native": "^0.60.14",
    "@types/react-native-modal": "^4.1.1",
    "@types/react-native-navigation": "^2.0.0",
    "@types/react-native-vector-icons": "^6.4.2",
    "@types/react-navigation": "^3.0.8",
    "@types/react-redux": "^7.1.2",
    "@types/react-test-renderer": "^16.9.0",
    "@types/recompose": "^0.30.7",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-mock-store": "^1.0.1",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-preset-react-native": "4.0.1",
    "babel-preset-react-native-stage-0": "1.0.1",
    "babel-runtime": "6.26.0",
    "detox": "^14.3.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-cumul8": "1.4.56",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "15.9.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.7.0",
    "jest": "24.9.0",
    "jest-circus": "^24.9.0",
    "lint-staged": "9.2.5",
    "metro-react-native-babel-preset": "^0.56.0",
    "mock-async-storage": "^2.2.0",
    "prettier": "1.18.2",
    "react-devtools": "3.6.3",
    "react-dom": "16.9.0",
    "react-native-dotenv": "0.2.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.9.0",
    "redux-logger": "3.0.6",
    "redux-mock-store": "^1.5.3",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Nexus_5X_API_29_x86"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "name": "Nexus_5X_API_29_x86"
      },
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/interview.app",
        "build": "xcodebuild -workspace ios/Interview.xcworkspace -scheme Interview -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    }
  }
}
